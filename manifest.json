{
    "manifest_version": 2,
  
    "name": "FormStash",
    "description": "This extension will stash form data and pop it back onto any page",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": ["tabs", "<all_urls>","storage"]
  }